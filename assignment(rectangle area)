// Online C compiler to run C program online
#include <stdio.h>

int main() {
    float side1,side2,area;
    printf("enter side1: ");
     scanf("%f",&side1);
     printf("enter side2: ");
     scanf("%f",&side2);
     area=side1*side2;
    printf("area of rectangle:%.2f \n",area);
    return 0;
}
