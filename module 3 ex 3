
#include <stdio.h>
#include <math.h>
void max();
void main()
{
    max();
}
void max()
{
int a,b;
printf("\nenter any number: ");
scanf("%d",&a);
printf("\nenter any number: ");
scanf("%d",&b);
if (a>b)
printf("\nthe maximum of given numbers=%d",a);
else
printf("\nthe maximum of given numbers=%d",b);
}
